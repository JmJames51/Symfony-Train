		<div>
			<h2>
				<a href='/category/{{ categories.name }}'>{{ categories.name }}</a>
			</h2>
		</div>
	{% endfor %}
	<div>
		<a href="{{ path("category_new")}}">
			Ajouter une nouvelle categorie</a>
	</div>
{% endblock %}
