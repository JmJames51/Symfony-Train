{% extends "base.html.twig" %}

{% block body %}

	<h1>{{category.name}}</h1>
	{% for program in programs %}

		<img class="image" src="{{ asset('upload/') ~ program.poster }}">
		<p>#{{program.id}}</p>
		<a href="{{ path("program_show", { program_id: program.id }) }}">{{program.title}}</a>
		<br>
		<p>{{program.synopsis}}</p>

	{% endfor %}

{% endblock %}
